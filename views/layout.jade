!!! 5
html(lang="en")
	head
		meta(charset="utf-8")
		meta(http-equiv="Content-Type", content="text/html; charset=UTF-8")
		meta(name="viewport", content="width=device-width, initial-scale=1.0, user-scalable=no")
		meta(name="description", content="")
		meta(name="author", content="")
		title Everybody's Google
		// link(rel='stylesheet', href='/bootstrap-ui/css/bootstrap.css')
		link(rel='stylesheet', href='/bootstrap-ui/css/bootstrap-responsive.css')
		// link(rel='stylesheet', href='/stylesheets/overrides.css')
		style(type="text/css")
			body { font-family: Arial, Verdana, sans-serif; font-size: 13px; }
			#tabs {
				height: 30px;
				width: 100%;
				z-index: 99999;
				top: 0;
				left: 0;
				background-color: #000;
				color: #ffffa9;
				position: absolute;
				font-size: 13px;
				font-family: Arial, sans-serif;
				font-weight: bold;
			}
			#tabs ul,
			#tabs ul li {
				height: 27px;
				float: left;
				margin: 0;
				padding: 0;
				list-style-type: none;
			}
			#tabs ul li a {
				color: #fcfffd;
				padding: 1px 2px 2px;
				margin-right: 16px;
				display: block;
				height: 27px;
				line-height: 27px;
				float: left;
				text-decoration: none;
				cursor: pointer;
			}
			#tabs ul li a:hover,
			#tabs ul li.active a {
				background-color: #888;
			}
			#tabs ul li.active a {
				background-color: #444;
			}
			#tabs ul li.active a:hover {
				background-color: #888;
			}
			#wrapper {
				margin-top: 64px;
			}
			body #content {
				width: 800px;
				margin: 0 auto;
				text-align: center;
			}
			body #content #searchbox {
				display: block;
				left: 0;
				border: 1px solid #ccc;
				padding: 0 0 0 2px;
				margin: 0 auto;
				width: 486px;
				height: 29px;
				text-align: center;
			}
			.search {
				width: 25%;
				float: left;
			}

			#searchBox {
				width:552px;height:27px;line-height:27px;font-size:13px;border:1px solid #C0C0C0;
			}

			.button {
				font-family: 'Lucida Grande';
				font-size: 11px;
				font-style: normal;
				font-variant: normal;
				font-weight: bold;
				border-bottom-color: rgba(0, 0, 0, 0.0976563);border-bottom-left-radius: 2px;border-bottom-right-radius: 2px;border-bottom-style: solid;border-bottom-width: 1px;background-attachment: scroll;background-clip: border-box;background-color: whiteSmoke;background-image: -webkit-linear-gradient(top, whiteSmoke, #F1F1F1);background-origin: padding-box;border-left-color: rgba(0, 0, 0, 0.0976563);border-left-style: solid;border-left-width: 1px;border-right-color: rgba(0, 0, 0, 0.0976563);border-right-style: solid;border-right-width: 1px;border-top-color: rgba(0, 0, 0, 0.0976563);border-top-left-radius: 2px;border-top-right-radius: 2px;border-top-style: solid;border-top-width: 1px;
				color: #444;
				display:inline-block;
				height: 29px;
				line-height: 29px;
				padding:0 8px;
			}
			a.button { text-decoration: none; }
			
			
			
			@media (max-width: 480px) {
				body #content { width: 100%; max-width: 480px; }
				.logo img { width: 250px; height: 103px; }
				#searchBox { width:80%; }
			}
	- if (typeof(bodyClasses) == 'undefined')
		- bodyClasses = ""
	body(class=bodyClasses)
		#tabs
			ul.navigation
				li: a(href="/") home
				li: a(href="/about") about
				li: a(href="/visualize") visualize
		#wrapper
			#header
				//h1 #{siteName}
			#content.container!= body


			
			#footer